{
    "name":              "gpratique/maison-construction",
    "description":       "Skeleton Application for Zend Framework zend-mvc applications",
    "type":              "project",
    "license":           "BSD-3-Clause",
    "keywords":          [
        "framework",
        "mvc",
        "zf"
    ],
    "homepage":          "http://framework.zend.com/",
    "minimum-stability": "alpha",
    "prefer-stable":     true,
    "config":            {
        "cache-dir": "/tmp/cache/.composer"
    },
    "require":           {
        "php":                                    ">= 7.1",
        "zendframework/zend-component-installer": "^1.0 || ^0.5 || ^1.0.0-dev@dev",
        "zendframework/zend-mvc":                 "^3.0.4",
        "zendframework/zendframework":            "^3.0.0",
        "oxcom/zend-twig":                        "^v0.1.3"
    },
    "require-dev":       {
        "ppito/zf3-whoops":                   "^1.3.0",
        "theseer/phpdox":                     "^0.10.1",
        "sebastian/phpcpd":                   "^3.0",
        "phpmd/phpmd":                        "^2.6",
        "pdepend/pdepend":                    "^2.5",
        "phploc/phploc":                      "^3.0",
        "squizlabs/php_codesniffer":          "^2.8",
        "phpunit/phpunit":                    "^6.0.10",
        "phing/phing":                        "^2.16",
        "leafo/scssphp":                      "^0.6.7",
        "zfcampus/zf-development-mode":       "^3.1.0",
        "zendframework/zend-developer-tools": "^1.1.1"
    },
    "autoload":          {
        "psr-4": {
        }
    },
    "autoload-dev":      {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/"
        }
    },
    "extra":             [],
    "scripts":           {
        "dev-off":        "zf-development-mode disable",
        "dev-on":         "zf-development-mode enable",
        "dev-status":     "zf-development-mode status",
        "fixedRight":     "sudo chown -R www-data: . && sudo chmod -R g+w .",
        "phing":          "phing"
    }
}
